include partials/google_analytics.pug
doctype html
html
    head
        +google-analytics-mixin()
        title Ganbreeder
        meta(charset="utf-8")
        meta(content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport")
        link(href="index.css" rel="stylesheet")
    body
        - var root = 'https://s3.amazonaws.com/ganbreederpublic/imgs/';
        header
            h1 Ganbreeder
            .text_container(style='text-align:center;')
                a.center(href='/', style='') Home
            h3 Your Starred Images
        .image_container.content
        #footer
        script.
            const root = "#{root}"
        script(src='/starred.js')